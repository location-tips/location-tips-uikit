{
  "name": "uikit-react",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "uikit-react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "uikit-react/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/uikit-react",
        "configDir": "uikit-react/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "uikit-react/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "options": {
          "configFile": "uikit-react/vite.config.ts",
          "outputPath": "dist"
      }
    }
  }
}
